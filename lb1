#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int count=0;
    for(int i=1;i<=n;i++)
    {
        int count=i;
        for(int j=i;j>=1;j--)
        {
            cout<<count<<" ";
            count--;
        }
        cout<<endl;
    }
    return 0;
}
/----------------------------------------------------------------------------------/
1
2 1
3 2 1
4 3 2 1
