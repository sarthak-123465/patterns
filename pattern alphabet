#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int count=1;
    for(int i=1;i<=n;i++)
    {
      // int count=i;
       
        for(int j=1;j<=n;j++)
        {
           
            cout<<char(count+64)<<" ";
           count++;
        }
        cout<<endl;
    }
    return 0;
}
